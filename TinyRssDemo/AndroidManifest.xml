<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tinymission.rssdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />
    
    <uses-permission android:name="android.permission.INTERNET" />
    <instrumentation android:name="android.test.InstrumentationTestRunner" android:targetPackage="com.tinymission.rssdemo"></instrumentation>

    <application
        android:icon="@drawable/tiny_circles"
        android:label="@string/app_name" >
        
    	<!-- <uses-library android:name="com.tinymission.rss" android:required="true"/> -->
    	
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/TinyTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name=".SimpleActivity"
            android:theme="@style/TinyTheme" >
        </activity>
        
        <activity
            android:name=".AdvancedActivity"
            android:theme="@style/TinyTheme" >
        </activity>
        
        <uses-library android:name="android.test.runner" android:required="true"/>
    </application>

</manifest>